// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "mysql"
  url      = env("DSN")
}

generator client {
  provider = "prisma-client-js"
}

model Example {
  id   Int     @id @default(autoincrement())
  name String?
}

enum Element {
  GRAPH_VISITORS_DEMOGRAPHIC
  GRAPH_VISITORS_STATISTICS
  GRAPH_SHARES_STATISTICS
  GRAPH_FOLLOWERS_DEMOGRAPHIC
  GRAPH_FOLLOWERS_STATISTICS
  TABLE_POSTS
}

enum Edge {
  REACTIONS
  METRICS
  VIDEO_ANALYTICS
  MEDIA
}

enum Dimension {
  REGION
  COUNTRY
  SENIORITY
  INDUSTRY
  FUNCTION
}
