{
  "name": "linkedin-tl",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run prisma:validate && npm run prisma:generate && npm run build:ts",
    "build:ts": "tsc",
    "start": "concurrently -k -p \"[{name}]\" -n \"Ms:Task:WebServer,Ms:Task:00\" -c \"red.bold,green.bold,blue.bold,magenta.bold,cyan.bold,gray.bold\" \"npm run start:task:web\" \"npm run start:task:00\\",
    "start:task:web": "node dist/index.js",
    "start:task:00": "node dist/Application/EventHandler/Task00/handler.js",
    "start-dev": "concurrently -k -p \"[{name}]\" -n \"Ms:Task:WebServer,Ms:Task:00\" -c \"red.bold,green.bold,blue.bold,magenta.bold,cyan.bold,gray.bold\" \"npm run watch:task:web\" \"npm run watch:task:00\\",
    "start-dev:task:web": "ts-node src/index.ts",
    "start-dev:task:00": "ts-node src/Application/EventHandler/Task00/handler.ts",
    "watch:ts": "npx tsc -w",
    "watch:task:web": "nodemon src/index.ts",
    "watch:task:00": "nodemon src/Application/EventHandler/Task00/handler.ts",
    "watch:prisma:generate": "npx prisma generate --watch",
    "prisma:introspect": "npx prisma introspect",
    "prisma:validate": "npx prisma validate",
    "prisma:generate": "npx prisma generate",
    "db:create": "npx prisma db push --accept-data-loss --force-reset",
    "prettier": "npx prettier --write .",
    "db:migration:up": "npx sequelize-cli db:migrate",
    "db:migration:down": "npx sequelize-cli db:migrate:undo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/jaeger-client": "^3.18.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "prisma": "^2.25.0",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@prisma/client": "^2.25.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "jaeger": "^1.0.0",
    "jaeger-client": "^3.18.1",
    "joi": "^17.4.0",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.1.0",
    "opentracing": "^0.14.5",
    "winston": "^3.3.3"
  }
}
