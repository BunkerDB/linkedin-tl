{
  "name": "linkedin-tl",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build:ts",
    "build:ts": "tsc",
    "ts:lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "concurrently -k -p \"[{name}]\" -n \"Ms:Task:WebServer,Ms:Task:00\" -c \"red.bold,green.bold,blue.bold,magenta.bold,cyan.bold,gray.bold\" \"npm run start:task:web\" \"npm run start:task:00\"",
    "start:task:web": "node dist/index.js",
    "start:task:00": "node dist/Application/EventHandler/Task00/handler.js",
    "start-dev": "concurrently -k -p \"[{name}]\" -n \"Ms:Task:WebServer,Ms:Task:00\" -c \"red.bold,green.bold,blue.bold,magenta.bold,cyan.bold,gray.bold\" \"npm run start-dev:task:web\" \"npm run start-dev:task:00\"",
    "start-dev:task:web": "ts-node src/index.ts",
    "start-dev:task:00": "ts-node src/Application/EventHandler/Task00/handler.ts",
    "watch:ts": "npx tsc -w",
    "watch:task:web": "nodemon src/index.ts",
    "watch:task:00": "nodemon src/Application/EventHandler/Task00/handler.ts",
    "prettier": "npx prettier --write .",
    "start-dev:command:kafka:topics:partition": "ts-node src/Application/Commands/Kafka/CreatePartitionTopic.ts",
    "start-dev:command:kafka:consumer_group_offsets:reset": "ts-node src/Application/Commands/Kafka/ResetConsumerGroupsOffsets.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/http-errors": "^1.8.0",
    "@types/jaeger-client": "^3.18.1",
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.17.3",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "concurrently": "^5.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "jaeger": "^1.0.0",
    "jaeger-client": "^3.18.1",
    "joi": "^17.4.0",
    "kafkajs": "^1.15.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.1.0",
    "opentracing": "^0.14.5",
    "qs": "^6.10.1",
    "winston": "^3.3.3"
  }
}
